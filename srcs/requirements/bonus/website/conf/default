server {
	listen 9090 default_server;
	listen [::]:9090 default_server;

	server_name localhost gwolf.42.fr;

	root /var/www/html;

	location /static_website/ {
		index index.html;
		# First attempt to serve request as file, then
		# as directory, then fall back to displaying a 404.
		try_files $uri $uri/ =404;
	}
}
